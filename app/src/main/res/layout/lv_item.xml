<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF">

    <TextView
        android:id="@+id/hodoroaga_ionut_question_edit_text"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:editable="false"
        android:ems="10"
        android:textColor="@color/black"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="Deprecated"
        tools:inputType="none" />

    <CheckBox
        android:id="@+id/hodoroaga_ionut_checkBox1"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="8dp"
        android:text="@string/checkbox"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.139"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/hodoroaga_ionut_question_edit_text" />

    <CheckBox
        android:id="@+id/hodoroaga_ionut_checkBox2"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="@string/checkbox"
        app:layout_constraintEnd_toEndOf="@id/hodoroaga_ionut_checkBox1"
        app:layout_constraintStart_toStartOf="@id/hodoroaga_ionut_checkBox1"
        app:layout_constraintTop_toBottomOf="@+id/hodoroaga_ionut_checkBox1" />

    <CheckBox
        android:id="@+id/hodoroaga_ionut_checkBox3"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="@string/checkbox"
        app:layout_constraintEnd_toEndOf="@id/hodoroaga_ionut_checkBox2"
        app:layout_constraintStart_toStartOf="@id/hodoroaga_ionut_checkBox2"
        app:layout_constraintTop_toBottomOf="@id/hodoroaga_ionut_checkBox2" />

    <CheckBox
        android:id="@+id/hodoroaga_ionut_checkBox4"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="@string/checkbox"
        app:layout_constraintEnd_toEndOf="@id/hodoroaga_ionut_checkBox2"
        app:layout_constraintStart_toStartOf="@id/hodoroaga_ionut_checkBox3"
        app:layout_constraintTop_toBottomOf="@+id/hodoroaga_ionut_checkBox3" />

</androidx.constraintlayout.widget.ConstraintLayout>